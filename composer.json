{
  "name": "lox24eu/lox24_api_client",
  "description": "LOX24 SMS GatewayAPI v2 official client",
  "minimum-stability": "stable",
  "version": "0.2.2",
  "type": "library",
  "license": "MIT",
  "support": {
    "email": "berlin@lox24.eu",
    "issues": "https://github.com/lox24/api_client_php/issues",
    "docs": "https://doc.lox24.eu"
  },
  "keywords": ["sms", "client", "lox24"],
  "require": {
    "php": ">=8.2",
    "psr/http-client": "1.x",
    "psr/http-message": "2.x",
    "psr/http-factory": "1.x",
    "psr/log": "3.x"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "dg/bypass-finals": "^1.5",
    "phpstan/phpstan": "^1.10"
  },
  "autoload": {
    "psr-4": {
      "lox24\\api_client\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "lox24\\api_client_tests\\": "tests/"
    }
  }
}
